include /etc/sway/config.d/*
include "${HOME}/.local/lib/sway.conf"

output eDP-1 enable pos 0 1920 res 1920x1080

set {
    $KEY_WIN                    Mod1
    $KEY_ALT                    Mod4
    $KEY_CTL                    Control
}

# SwayFX Specific Configuration
