## Output Configuration
output eDP-1 enable position 0 1920 resolution 1920x1080 scale 1 scale_filter smart

output * {
    allow_tearing   no
    max_render_time off
}

## Input Configuration
input * {
    xkb_layout         "us"
    left_handed        disabled
    xkb_numlock        disabled
    xkb_capslock       disabled
    natural_scroll     disabled
    middle_emulation   disabled
    scroll_button_lock disabled
}
input type:touch {
    tool_mode     pen    absolute
    tool_mode     pencil absolute
    map_to_output eDP-1
}
input type:pointer {
    dwt           disabled
    dwtp          disabled
    drag_lock     disabled
    accel_profile adaptive
    pointer_accel -0.175
}
input type:touchpad {
    dwt                    enabled
    tap                    enabled
    drag                   enabled
    dwtp                   disabled
    drag_lock              disabled
    accel_profile          adaptive
    pointer_accel          -0.175
    scroll_method          two_finger
    tap_button_map         lrm
    clickfinger_button_map lrm
}

## Seat Configuration
seat * {
    hide_cursor         0
    xcursor_theme       MoonlightSky 24
    keyboard_grouping   smart
    pointer_constraint  enable
    shortcuts_inhibitor enable
}

## Variable Configuration
set {
    $KEY_ALT Mod4
    $KEY_CTL Control
    $KEY_WIN Mod1
}

