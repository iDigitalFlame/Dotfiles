    "$schema": "/etc/xdg/swaync/configSchema.json",
    "control-center-exclusive-zone": true,
    "control-center-height": 300,
    "control-center-layer": "top",
    "control-center-margin-bottom": 5,
    "control-center-margin-left": 0,
    "control-center-margin-right": 5,
    "control-center-margin-top": 5,
    "control-center-width": 450,
    "cssPriority": "user",
    "fit-to-screen": true,
    "hide-on-action": true,
    "hide-on-clear": true,
    "image-visibility": "when-available",
    "keyboard-shortcuts": true,
    "layer": "overlay",
    "layer-shell": true,
    "notification-2fa-action": false,
    "notification-body-image-height": 200,
    "notification-body-image-width": 200,
    "notification-icon-size": 48,
    "notification-inline-replies": false,
    "notification-window-width": 400,
    "positionX": "right",
    "positionY": "top",
    "relative-timestamps": true,
    "script-fail-notify": false,
    "timeout": 5,
    "timeout-critical": 0,
    "timeout-low": 5,
    "transition-time": 200,
    "widget-config": {
        "backlight": {
            "device": "intel_backlight",
            "label": "",
            "min": 10,
            "subsystem": "backlight"
        },
        "buttons-grid": {
            "actions": [
                {
                    "label": "",
                    "command": "sh -c 'swaync-client -cp; ${HOME}/.local/bin/sway/clip copy'"
                },
                {
                    "label": "",
                    "command": "sh -c 'swaync-client -cp; ${HOME}/.local/bin/sway/shot copy'"
                },
                {
                    "label": "",
                    "command": "sh -c 'swaync-client -cp; ${HOME}/.local/bin/hview'"
                },
                {
                    "label": "",
                    "command": "sh -c 'swaync-client -cp; ${HOME}/.local/bin/sway/clip'"
                },
                {
                    "label": "",
                    "command": "sh -c 'swaync-client -cp; ${HOME}/.local/bin/sway/shot'"
                }
            ]
        },
        "dnd": {
            "text": " Do Not Disturb"
        },
        "menubar#ctl": {
            "menu#locker": {
                "actions": [
                    {
                        "label": " Clear All Lockers",
                        "command": "sh -c 'swaync-client -cp; lockerctl -kz false -kb false -kl false -ks false'"
                    },
                    {
                        "label": " Clear All Lockers (Include System)",
                        "command": "sh -c 'swaync-client -cp; lockerctl -kz false -kb false -kl false -ks false -kl false -kd false'"
                    },
                    {
                        "label": " Coffee (All 5m)",
                        "command": "sh -c 'swaync-client -cp; lockerctl -z 300 -kb 300 -kl 300 -s 300'"
                    },
                    {
                        "label": " Caffeine (All 10m)",
                        "command": "sh -c 'swaync-client -cp; lockerctl -z 600 -kb 600 -kl 600 -s 600'"
                    },
                    {
                        "label": " Cocaine (All 15m)",
                        "command": "sh -c 'swaync-client -cp; lockerctl -z 900 -kb 900 -kl 900 -s 900'"
                    },
                    {
                        "label": " Insomnia (All Unlimited)",
                        "command": "sh -c 'swaync-client -cp; lockerctl -kz true -kb true -kl true -ks true'"
                    },
                    {
                        "label": " Disable Screen Blank",
                        "command": "sh -c 'swaync-client -cp; lockerctl -kb true'"
                    },
                    {
                        "label": " Disable Screen Lock",
                        "command": "sh -c 'swaync-client -cp; lockerctl -kl true'"
                    },
                    {
                        "label": " Disable Sleep",
                        "command": "sh -c 'swaync-client -cp; lockerctl -ks true'"
                    },
                    {
                        "label": " Disable Hibernation",
                        "command": "sh -c 'swaync-client -cp; lockerctl -kz true'"
                    },
                    {
                        "label": " Disable Lid Switch",
                        "command": "sh -c 'swaync-client -cp; lockerctl -kd true'"
                    },
                    {
                        "label": " Disable Yubikey",
                        "command": "sh -c 'swaync-client -cp; lockerctl -ky true'"
                    }
                ],
                "animation-type": "slide_down",
                "label": "",
                "position": "left"
            },
            "menu#cpu": {
                "actions": [
                    {
                        "label": " Power Saving (Max 600Mhz)",
                        "command": "sh -c 'swaync-client -cp; cpuctl -x 600Mhz -m 400Mhz -t 0 -tx 20 -tm 10 -g powersave -p power'"
                    },
                    {
                        "label": " Less Savings (Max 800Mhz)",
                        "command": "sh -c 'swaync-client -cp; cpuctl -x 800Mhz -m 400Mhz -t 0 -tx 20 -tm 10 -g powersave -p balance_power'"
                    },
                    {
                        "label": " Semi-Balanced (Max 1.5Ghz)",
                        "command": "sh -c 'swaync-client -cp; cpuctl -x 1.5Ghz -m 400Mhz -t 0 -tx 40 -tm 10 -g powersave -p balance_power'"
                    },
                    {
                        "label": " Balanced (Max 2.5Ghz)",
                        "command": "sh -c 'swaync-client -cp; cpuctl -x 2.5Ghz -m 400Mhz -t 1 -tx 70 -tm 10 -g performance -p balance_performance'"
                    },
                    {
                        "label": " Power (Max 3Ghz, Turbo)",
                        "command": "sh -c 'swaync-client -cp; cpuctl -x 3.0Ghz -m 400Mhz -t 1 -tx 85 -tm 20 -g performance -p performance'"
                    },
                    {
                        "label": " Max Power (Max 4.8Ghz, Turbo)",
                        "command": "sh -c 'swaync-client -cp; cpuctl -x 4.8Ghz -m 400Mhz -t 1 -tx 100 -tm 10 -g performance -p performance'"
                    }
                ],
                "animation-type": "slide_down",
                "label": "",
                "position": "left"
            },
            "menu#power": {
                "actions": [
                    {
                        "label": " Suspend",
                        "command": "sh -c 'swaync-client -cp; sudo systemctl suspend'"
                    },
                    {
                        "label": " Hibernate",
                        "command": "sh -c 'swaync-client -cp; sudo systemctl hibernate'"
                    },
                    {
                        "label": " Reboot",
                        "command": "sudo systemctl reboot"
                    },
                    {
                        "label": " Shut Down",
                        "command": "sudo systemctl poweroff"
                    }
                ],
                "animation-type": "slide_down",
                "label": "",
                "position": "right"
            },
            "buttons": {
                "actions": [
                    {
                        "label": "",
                        "command": "sh -c 'swaync-client -cp; bluectl -t'"
                    },
                    {
                        "label": "",
                        "command": "sh -c 'swaync-client -cp; wlay'"
                    },
                    {
                        "label": "",
                        "command": "sh -c 'swaync-client -cp; lockctl -f'"
                    }
                ],
                "position": "right"
            }
        },
        "title": {
            "button-text": "",
            "clear-all-button": true,
            "text": ""
        },
        "volume": {
            "label": "",
            "show-per-app": false,
            "show-per-app-icon": false,
            "show-per-app-label": false
        }
    },
    "widgets": [
        "menubar#ctl",
        "volume",
        "backlight",
        "dnd",
        "title",
        "notifications",
        "buttons-grid"
    ]
}