{
    "background": {
        "auto_change": 600,
        "directory": "/home/iDigitalFlame/Pictures/Backgrounds",
        "lockscreen": "/home/iDigitalFlame/.cache/smd/lockscreen.png",
        "screen_size": "1920x1080"
    },
    "hydra": {
        "aliases": {
            "mac": "/opt/hydra/iDigitalFlame/MacOS/vm.conf",
            "win": "/opt/hydra/iDigitalFlame/W10D/vm.conf",
            "win10": "/opt/hydra/iDigitalFlame/W10D/vm.conf",
            "win7": "/opt/hydra/iDigitalFlame/Windows7-iOS/Windows7-iOS.conf"
        },
        "directory": "/opt/hydra/iDigitalFlame"
    },
    "locker": {
        "command": "/home/iDigitalFlame/.local/bin/i3/lock",
        "hibernate": 180,
        "lid_bypass": true,
        "lockscreen": 120,
        "screen_blank": 60,
        "suspend": 60
    },
    "notify": {
        "default": "dialog-information.svg",
        "dirs": [
            "/usr/share/icons/korla/apps/scalable"
        ],
        "theme": "/usr/share/icons/korla/status/scalable"
    },
    "polybar": {
        "bars": {
            "inverted": "",
            "left": "small",
            "normal": "",
            "right": "small"
        },
        "default": "standard"
    },
    "profile": {
        "hibernate_post": [],
        "hibernate_pre": [],
        "lock_post": [],
        "lock_pre": [],
        "power_ac": [
            "/usr/local/bin/lockerctl -z true",
            "/usr/local/bin/cpuctl -x 4.0Ghz -m 400Mhz -t 1 -tx 100 -tm 20 -g performance -p performance"
        ],
        "power_battery": [
            "/usr/local/bin/lockerctl -kz false",
            "/usr/local/bin/cpuctl -x 800Mhz -m 400Mhz -t 0 -tx 20 -tm 10 -g powersave -p balance_power"
        ],
        "rotate": [],
        "suspend_post": [],
        "suspend_pre": [],
        "video": []
    },
    "session": {
        "composer": [
            "/usr/bin/compton",
            "-cd",
            ":1",
            "--config",
            "/home/iDigitalFlame/.config/theme/current/compton.conf"
        ],
        "startups": [
            "/usr/bin/touchegg",
            "/usr/bin/nm-applet",
            "/usr/bin/onboard --size=800x300",
            "/usr/lib/smd/libexec/smd-enable-tap",
            "/usr/bin/sg firewall-web -c /usr/bin/nextcloud",
            "/usr/bin/gnome-keyring-daemon --start --components=secrets,ssh"
        ]
    }
}