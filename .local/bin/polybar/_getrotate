#!/usr/bin/bash
################################
###### iDigitalFlame 2019 ######
#                              #
#            -/`               #
#            -yy-   :/`        #
#         ./-shho`:so`         #
#    .:- /syhhhh//hhs` `-`     #
#   :ys-:shhhhhhshhhh.:o- `    #
#   /yhsoshhhhhhhhhhhyho`:/.   #
#   `:yhyshhhhhhhhhhhhhh+hd:   #
#     :yssyhhhhhyhhhhhhhhdd:   #
#    .:.oyshhhyyyhhhhhhddd:    #
#    :o+hhhhhyssyhhdddmmd-     #
#     .+yhhhhyssshdmmddo.      #
#       `///yyysshd++`         #
#                              #
########## SPACEPORT ###########
################################
## Rotate Status Command
# iDigitalFlame

if ! [ -f "/var/run/smd/rotate" ]; then
    printf "\n"
    exit 0
fi

case $(cat /var/run/smd/rotate) in
    "true")
    printf "\n"
    ;;
    "false")
    printf "\n"
    ;;
    *)
    printf "\n"
    ;;
esac
exit 0
