{
    "background_size": "1920x1080",
    "background_time": 600,
    "backgrounds": "/home/iDigitalFlame/Pictures/Backgrounds",
    "composer": [
        "/home/iDigitalFlame/.local/bin/compton",
        "-cd",
        ":1",
        "--config",
        "/home/iDigitalFlame/.config/theme/current/compton.conf",
        "--blur-method",
        "kawase",
        "--blur-strength",
        "3"
    ],
    "hydra_aliases": {
        "android": "/home/iDigitalFlame/Documents/VMs/Android7/Android7.conf",
        "ios": "/home/iDigitalFlame/Documents/VMs/Windows7-iOS/Windows7-iOS.conf",
        "kali": "/home/iDigitalFlame/Documents/VMs/KaliLive.conf",
        "win": "/home/iDigitalFlame/Documents/VMs/Windows10/Windows10.conf",
        "win10": "/home/iDigitalFlame/Documents/VMs/Windows10/Windows10.conf"
    },
    "hydra_directory": "/home/iDigitalFlame/Documents/VMs",
    "locker": [
        "/home/iDigitalFlame/.local/bin/i3/lock"
    ],
    "lockscreen_blank": 5,
    "polybar_bars": {
        "laptop": "laptop",
        "tablet": "tablet"
    },
    "profiles": {
        "display": [
            "/usr/bin/xrandr --dpi 95x95"
        ],
        "lock-post": [],
        "lock-pre": [],
        "mode-laptop": [
            "/usr/bin/killall onboard"
        ],
        "mode-tablet": [
            "/usr/bin/onboard"
        ],
        "power-ac": [
            "/opt/spaceport/bin/cpuctl -x 3.0Ghz -m 400 -t 1 -tx 85 -tm 20 -g performance -p performance"
        ],
        "power-battery": [
            "/opt/spaceport/bin/cpuctl -x 800 -m 400 -t 0 -tx 20 -tm 10 -g powersave -p balance_power"
        ],
        "rotate": [],
        "sleep-post": [],
        "sleep-pre": []
    },
    "startup": [
        "/usr/bin/nm-applet",
        "/usr/bin/libinput-gestures",
        "/opt/spaceport/libexec/smd-diskd",
        "/opt/spaceport/libexec/smd-enable-tap",
        "/usr/bin/sg iptables-web -c /usr/bin/nextcloud",
        "/usr/bin/gnome-keyring-daemon --start --components=secrets,ssh"
    ],
    "timeout_blank": 60,
    "timeout_lock": 120,
    "timeout_sleep": 120
}